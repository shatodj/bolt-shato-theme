{% extends "twig/partials/_navbar.twig" %}

{% set useSearbar = true %}

{% block navbar_last_item %}
    {# searchbar#}
    {% if useSearchbar %}
        {% include "twig/partials/_sub_search_form.twig" %}
    {% endif %}
    
    {# custom menu bar #}
    {% apply spaceless %}
        {{ menu('social-navbar', 'twig/partials/_sub_menu_social.twig') }}
    {% endapply %}

    {{ widgets('main_top') }}
{% endblock %}